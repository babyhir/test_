pipeline {
	agent any
	stages {
		stage ('Build') {
			steps {
				echo "Build"
				echo "Test"
				echo "Integration Test"
			}
		}
		stage ('Test') {
			steps {
				echo "Build"
				echo "Test"
				echo "Integration Test"
			}
		}
		stage ('Integration') {
			steps {
				echo "Build"
				echo "Test"
				echo "Integration Test"
			}
		}
	}
}

